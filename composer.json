{
    "type": "project",
    "name": "survos-sites/movies",
    "description": "Started out as a movie demo, but now several meilisearch demos",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^4.3.4",
        "doctrine/doctrine-bundle": "^3.0",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5.7",
        "easycorp/easyadmin-bundle": "4.x-dev",
        "halaxa/json-machine": "^1.2.5",
        "league/csv": "^9.27.1",
        "liquid/liquid": "^1.4.44",
        "meilisearch/meilisearch-php": "dev-main",
        "openai-php/client": "^0.18.0",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "survos/ez-bundle": "^2.0",
        "survos/import-bundle": "^2.0.56",
        "survos/installer": "^2.1.4",
        "survos/jsonl-bundle": "^2.0",
        "survos/meili-bundle": "^2.0.56",
        "symfony/asset": "^7.3 | ^8.0",
        "symfony/asset-mapper": "^7.3.4 | ^8.0",
        "symfony/console": "^7.3.5 | ^8.0",
        "symfony/doctrine-messenger": "^7.3.4 | ^8.0",
        "symfony/dotenv": "^7.3.2 | ^8.0",
        "symfony/expression-language": "^7.3.2 | ^8.0",
        "symfony/flex": "^2.10.0",
        "symfony/form": "^7.3.5 | ^8.0",
        "symfony/framework-bundle": "^7.3.5 | ^8.0",
        "symfony/http-client": "^7.3.4 | ^8.0",
        "symfony/intl": "^7.3.5 | ^8.0",
        "symfony/mailer": "^7.3.4 | ^8.0",
        "symfony/mime": "^7.3.4 | ^8.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3.3 | ^8.0",
        "symfony/process": "^7.3.4 | ^8.0",
        "symfony/property-access": "^7.3.3 | ^8.0",
        "symfony/property-info": "^7.3.4 | ^8.0",
        "symfony/runtime": "^7.3.4 | ^8.0",
        "symfony/security-bundle": "^7.3.4 | ^8.0",
        "symfony/serializer": "^7.3.4 | ^8.0",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "^7.4",
        "symfony/translation": "^7.3.4 | ^8.0",
        "symfony/twig-bundle": "^7.3.4 | ^8.0",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-turbo": "^2.31",
        "symfony/validator": "^7.3.4 | ^8.0",
        "symfony/web-link": "^7.3 | ^8.0",
        "symfony/yaml": "^7.3.3 | ^8.0",
        "twig/extra-bundle": "^2.12|^3.22.1",
        "twig/twig": "^2.12|^3.22.0",
        "yethee/tiktoken": "^0.11.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4.3",
        "roave/security-advisories": "dev-latest",
        "survos/code-bundle": "^2.0.55",
        "survos/deployment-bundle": "^2.0",
        "symfony/browser-kit": "^7.3.2 | ^8.0",
        "symfony/css-selector": "^7.3 | ^8.0",
        "symfony/debug-bundle": "^7.3.4 | ^8.0",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "^7.3 | ^8.0",
        "symfony/web-profiler-bundle": "^7.3.4 | ^8.0"
    }
}
